{
  "name": "dataform-docs",
  "version": "1.0.0",
  "description": "Beautiful documentation generator for Dataform projects",
  "main": "dist/cli/index.js",
  "bin": {
    "dataform-docs": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "node_modules/.bin/tsc && npm run build:site",
    "build:site": "cd site-generator && npm run build && cp -r dist/* ../test-docs/",
    "dev": "node_modules/.bin/tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "node_modules/.bin/tsc --noEmit"
  },
  "files": [
    "dist/",
    "src/site/"
  ],
  "keywords": [
    "dataform",
    "documentation",
    "data-warehouse",
    "sql",
    "dag",
    "analytics"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "express": "^4.18.2",
    "glob": "^10.3.0",
    "open": "^9.1.0",
    "puppeteer": "^24.19.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
